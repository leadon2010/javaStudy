<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectfx.controller.TableViewController"
	prefHeight="180" prefWidth="500">
	<Label layoutX="11" layoutY="9" text="TableView" />
	<TableView fx:id="tableView" layoutX="120" layoutY="30" prefHeight="100" prefWidth="290">
		<columns>
			<TableColumn prefWidth="100" text="FirstName" />
			<TableColumn prefWidth="100" text="LastName" />
			<TableColumn prefWidth="100" text="Salary" />
		</columns>
	</TableView>
	<Button layoutX="190" layoutY="145" onAction="#handleBtnOkAction" text="확인" />
	<Button layoutX="260" layoutY="145" onAction="#handleBtnCancelAction" text="취소" />
</AnchorPane>

