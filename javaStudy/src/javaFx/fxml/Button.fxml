<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0"
	prefWidth="420.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<center>
		<HBox prefHeight="100.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<children>
				<VBox alignment="CENTER_LEFT" prefHeight="100.0"
					prefWidth="100.0" spacing="20.0">
					<children>
						<CheckBox mnemonicParsing="false" text="CheckBox" />
						<CheckBox mnemonicParsing="false" text="CheckBox" />
					</children>
				</VBox>
				<ImageView fitWidth="100.0" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="@../images/geek.gif" />
					</image>
				</ImageView>
				<Separator orientation="VERTICAL" prefHeight="100.0" />
				<VBox alignment="CENTER_LEFT" prefHeight="100.0"
					prefWidth="150.0" spacing="20.0">
					<children>
						<RadioButton mnemonicParsing="false"
							text="BubbleChart" />
						<RadioButton mnemonicParsing="false" text="BarChart" />
						<RadioButton mnemonicParsing="false" text="AreaChart" />
					</children>
				</VBox>
				<ImageView fitWidth="100.0" pickOnBounds="true"
					preserveRatio="true">
					<image>
						<Image url="@../images/BarChart.png" />
					</image>
				</ImageView>
			</children>
		</HBox>
	</center>
	<bottom>
		<Button fx:id="btnExit" BorderPane.alignment="CENTER"
			text="Button" BorderPane.alignment="CENTER" />
	</bottom>
</BorderPane>
